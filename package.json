{
    "name": "node-app",
    "version": "1.0.0",
    "description": "Backend for helsinki city bike app",
    "main": "server.ts",
    "scripts": {
        "dev": "NODE_ENV=development nodemon --ignore './src/runcsv.ts' --exec 'ts-node' ./src/server.ts",
        "start": "NODE_PATH=./dist node dist/server.ts",
        "build": "rm -f dist && npx tsc",
        "eslint:fix": "eslint --fix",
        "test": "echo \"Error: no test specified\" && exit 1",
        "csv": "NODE_OPTIONS='--max-old-space-size=3072' ts-node ./src/runcsv.ts",
        "nodemonConfig": {
            "ignore": [
                "./src/runcsv.ts",
                "./src/db/data/*.csv"
            ]
        }
    },
    "keywords": [
        "node"
    ],
    "author": "Rohit bhandari",
    "license": "ISC",
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/node": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.30.0",
        "@typescript-eslint/parser": "^5.30.0",
        "dotenv": "^16.0.1",
        "eslint": "^8.18.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.1.0",
        "nodemon": "^2.0.18",
        "ts-node": "^10.8.1",
        "typescript": "^4.7.4"
    },
    "dependencies": {
        "@types/pg": "^8.6.5",
        "cors": "^2.8.5",
        "express": "^4.18.1",
        "fast-csv": "^4.3.6",
        "pg": "^8.7.3",
        "reflect-metadata": "^0.1.13",
        "typeorm": "^0.3.7"
    }
}
